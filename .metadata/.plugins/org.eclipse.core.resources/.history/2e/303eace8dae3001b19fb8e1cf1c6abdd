package first;

public class forExam {

	public static void main(String[] args) {
		
		for(int i = 1; i <= 100; i++) {
			System.out.println(i);
		}
		
		System.out.println("\n");
		
		int total = 0;
		for(int i = 0; i<=100; i++) {
			if (i%2==0) {
				continue;
			}
			total += i;
		}
		System.out.println("0부터 100까지 홀수값의 합 : "+ total);
			
	}

}
