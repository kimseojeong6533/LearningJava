package first;
import java.util.Scanner;

public class doWhileExam {

	public static void main(String[] args) {
		
		int value = 0;
		Scanner scan = new Scanner(System.in); //System.in : 값을 입력 
		do {
			//우선 반복,
			value = scan.nextInt();
			System.out.println("입력받은 값 : " + value);
		}while(value != 10);
		
		System.out.println("반복문 종료!");
	}

}
