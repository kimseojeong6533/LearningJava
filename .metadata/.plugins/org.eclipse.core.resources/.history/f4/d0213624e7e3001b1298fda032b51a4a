package first;

public class arrayExam {

	public static void main(String[] args) {
		int[] array1 = new int[100];  // 배열 선언 (100개의 칸이 있는 배열을 생성)
		array1[0] = 50;     
		array1[10] = 100;
		
		int[] array2 = new int[] {1,2,3,4};  //배열 선언
		
		int [] array3 = {1,2,3,4};               //배열 선언
		
		System.out.println(array3[0]);
		
		
		
		
		
				
		}
		
				
		//배열 전체 출력은 어떻게? 
		
		

	}

}
