package second;

// 인터페이스 디폴트 메소드를 사용해보자 (default void 메소드명((매개변수)) {}   

interface Printable{
	void print(String doc);
	default void printCMYK(String doc) {} // 인터페이스 디폴트 메소드 선언 
}

class Prn731Drv implements Printable{
	@Override
	public void print(String doc) {
		System.out.println("From MD-731 printer");
		System.out.println(doc);
		
	}
}

class Prn908Drv implements Printable{
	@Override
	public void print(String doc) {
		System.out.println("From Md-908 black & white ver");
	}
	
	@Override
	public void printCMYK(String doc) {
		System.out.println("From MD-908 CMYK ver");
		System.out.println(doc);
	}
}

public class PrinterDriver4 {

	public static void main(String[] args) {
		Printable prn1 = new Prn731Drv();
		Printable prn2 = new Prn908Drv();
		String myDoc = "This is Interface Exercise!!";
		
		prn1.print(myDoc);
		prn2.print(myDoc);
		System.out.println();
		prn2.printCMYK(myDoc);
	

	}

}
