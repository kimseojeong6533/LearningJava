public class BankAccountTest {

	public static void main(String[] args) {
		second.BankAccountconsructor kim = new second.BankAccountconsructor("12-34-89", "990990-9090990", 10000);
		kim.deposit(10000);
		kim.withdraw(5000);
		kim.checkMyBalance();
		
	}

}
