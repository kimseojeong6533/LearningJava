package second;

class SuperCLS{
	protected static int count = 0;  // 상속관계의 클래스에서 접근가능한 멤버변수 count
	
	public SuperCLS() {    // 인스턴스 또는 클래스이름으로 접근하면 1이 증가됨.
		count++;
	}
}

class SubCLS extends SuperCLS{
	SubCLS(){
		super();
	}
	public void showCount() {
		System.out.println("count : "+count);
	}
}

public class SuperSubStatic {

	public static void main(String[] args) {
		SuperCLS obj1 = new SuperCLS();
		SuperCLS obj2 = new SuperCLS();
		SubCLS obj3 = new SubCLS();
		obj3.showCount();
	}

}
